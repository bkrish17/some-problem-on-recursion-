#include <iostream>
using namespace std;
void printstar(int n)
{
    if(n==0)
        return;
    printstar(n-1);
    for(int i=0;i<n;i++)
    {
        cout<<"*";
    }
    cout<<endl;
       
}
int main()
{
    int n;
    cout<<"enter number of rows";
    cin>>n;
    printstar(n);
    return 0;
}
